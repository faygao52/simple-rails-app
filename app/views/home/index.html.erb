<h1>Welcome!</h1>

<% flash.each do |key, value| %>
    <%= content_tag :div, value, class: "flash #{key}" %>
  <% end %>
<%= form_with(url: { :controller => "users", :action => 'import' }, multipart: true) do |f| %>
    <%= f.label :file, "Please select a CSV file" %>
    <%= f.file_field :file, required: true %>
    <%= submit_tag( "Import" ) %>
<% end %>